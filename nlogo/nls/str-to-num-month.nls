; __includes ["check-string.nls" "match.nls"]

to-report str-to-num-month [#month]
  assert-string #month

  let months (list
    "January" "February" "March" "April" "May" "June"
    "July" "August" "September" "October" "November" "December"
  )

  let out match #month months

  ifelse (is-number? out) [
    report out + 1
  ] [
    report out
  ]
end
